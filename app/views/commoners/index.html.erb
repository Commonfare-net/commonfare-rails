<p id="notice"><%= notice %></p>

<h1>Commoners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commoners.each do |commoner| %>
      <tr>
        <td><%= commoner.name %></td>
        <td><%= link_to 'Show', commoner %></td>
      </tr>
    <% end %>
  </tbody>
</table>
