<%= content_for :title, s_('Title|New listing') %>
<div class="row justify-content-center">
  <div class="col-lg-7 col-md-9 py-4 bg-white">
    <h2 class="pt-4 pb-2"><%= s_('Title|New listing') %></h2>
    <%= simple_form_for(@listing) do |f| %>
      <%= f.error_notification %>
      <%= render 'fields', f: f %>
      <%= f.button :submit, s_('Button|Create listing'), class: 'btn btn-cf btn-block' %>
    <% end %>
  </div>
</div>
